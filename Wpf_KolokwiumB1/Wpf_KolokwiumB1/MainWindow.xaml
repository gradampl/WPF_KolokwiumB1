        <TextBox x:Name="nazwaDruzyny1" HorizontalAlignment="Left" Height="23" Margin="132,282,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="ustawNazwe" Content="Ustaw nazwę" HorizontalAlignment="Left" Margin="296,282,0,0" VerticalAlignment="Top" Width="75" Click="ustawNazwe_Click"/>
        <Label x:Name="druzyna" Content="Drużyna" HorizontalAlignment="Left" Margin="296,0,0,0" VerticalAlignment="Top"/>
